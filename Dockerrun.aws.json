{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [{
			"name": "client",
			"image": "rkudache/multi-client",
			"hostname": "client",
			"essential": false,
                        "memory": 128
		},
		{
			"name": "server",
			"image": "rkudache/multi-server",
			"hostname": "api",
			"essential": false,
                       "memory": 128
		},
		{
			"name": "worker",
			"image": "rkudache/multi-worker",
			"hostname": "worker",
			"essential": false,
                        "memory": 128
		},
		{
			"name": "nginx",
			"image": "rkudache/multi-nginx",
			"hostname": "nginx",
			"essential": true,
			"portMappings": [{
				"hostPort": 80,
				"containerPort": 80
			}],
			"links": ["server", "client"],
                         "memory": 128
		}
	]
}
